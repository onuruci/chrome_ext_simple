{
  "name": "Mirza Photo Change Extension",
  "description": "Change every image with mirza art",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["m.jpg"]
}]
}

